safe = (Y \safe.\x.\d.\qs.(qs true 
  \q.\l.[(and (x != q))
         (and (q + d != x))
         (and (q - d != x))
         (safe x (d + 1) l)]))
gen = \nq.(Y \gen.\n.(n = 0 (cons nil nil) (concatMap \b.(concatMap \q.(safe q 1 b 
  (cons (cons q b) nil) nil) (range 1 nq)) (gen (n - 1)))) nq)

main = [putStrLn showInt length gen 5]
