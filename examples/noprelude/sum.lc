+ = \n.\m.[m n \n.\m._+]
* = \n.\m.[m n \n.\m._*]
nil = \n.\c.n
cons = \h.\t.\n.\c.(c h t)
Y = \g.(\x.[g x x] \x.[g x x])
foldr = (Y \foldr.\f.\a.\bs.(bs a \b.\bs.(f b (foldr f a bs))))
sum = (foldr + 0)
prod = (foldr * 1)
sys_exit = \code.\w.[w code \a.(\t.\w._!1 60)]
main = (sys_exit {arr = "1234"}(+ (prod arr) (sum arr)))
